pipeline{
    agent{
        label 'prod'
    }
    tools{
        jdk 'java-11'
        maven 'maven'
    }
    stages{
        stage('git checkout') {
            steps {
                git branch: 'jenkins_file', url: 'https://github.com/metreshekhar/devops.git'
            }
        }
        stage('Compile') {
            steps {
                sh "mvn compile"
            }
        }
        stage('Build') {
            steps {
                sh "mvn clean package"
            }
        }
    }
}